{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Aktuelle Datei",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "env": { "PYTHONPATH": "${workspaceRoot}"},
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "args": ["--output_file", "./test/augmented.txt", "--output_file1", "./test/original.txt", "--skip_sentences", "0", "--block_size", "128"]
        }
    ]
}